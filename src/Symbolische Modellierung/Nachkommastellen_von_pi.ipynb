   "source": [
    "# Nachkommastellen von pi\n",
    "\n",
    "Geben Sie die 289. Nachkommastelle von π an."
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 4,
   "id": "919ffe2b",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Die 289. Nachkommastelle von π: 2\n"
     ]
    }
   ],
   "source": [
    "import mpmath\n",
    "\n",
    "mpmath.mp.dps = 300\n",
    "\n",
    "pi_value = mpmath.pi\n",
    "\n",
    "pi_str = str(pi_value)\n",
    "\n",
    "n = 289\n",
    "pi_289 = int(pi_str[n+1])\n",
    "\n",
    "print(f\"Die 289. Nachkommastelle von π: {pi_289}\")"
   ]
  }
